{
  "all": true,
  "check-coverage": true,
  "reporter": [
    "html",
    "text",
    "lcov",
    "text-summary"
  ],
  "statements": 80,
  "branches": 75,
  "functions": 80,
  "lines": 80,
  "include": [
    "Lib/**/*.js",
    "Controller/**/*.js",
    "Validation/**/*.js",
    "Presenter/**/*.js"
  ],
  "exclude": [
    "test/**",
    "node_modules/**",
    "coverage/**",
    "config/**",
    "Model/**"
  ],
  "cache": true,
  "report-dir": "./coverage"
}
